  <Variable
    Name="P_ADF_USER_ID"
    Kind="where"
    Type="java.lang.Integer">
    <AttrArray Name="ExtendedDataInts">
      <Item Value="0"/>
    </AttrArray>
  </Variable>
  <SQLQuery><![CDATA[SELECT  su.user_id,   
        su.user_code login_id,   
        su.user_name,   
        su.first_name,   
        su.last_name,   
        coalesce(su.email,'-') email,   
        coalesce(su.phone_no,'-') phone_no,   
        coalesce(su.mobile_no,'-') mobile_no,   
        coalesce(sc.cityname,'-') cityname,   
        sc.citycode,   
        sg.GENDER_DESCRIPTION   
FROM 	sys_users su   
left outer join   
sm_city sc   
on sc.citycode=su.city_sno    
left outer join   
set_gender sg   
on   
su.gender_sno=sg.GENDER_SNO   
where su.user_id=?]]></SQLQuery>
